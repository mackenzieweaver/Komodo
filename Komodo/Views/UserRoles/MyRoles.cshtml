@model IEnumerable<string>
@{
    ViewData["Title"] = "My Roles";
}

<h1>My Roles</h1>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            Role
        </dt>
        <dd class="col-sm-10">
            @foreach(var role in Model)
            {
                @role
            }
        </dd>
    </dl>
</div>
